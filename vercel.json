{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/starter", "dest": "/index.html?plan=starter" },
    { "src": "/pro",     "dest": "/index.html?plan=pro"     },
    { "src": "/elite",   "dest": "/index.html?plan=elite"   },
    { "src": "/(.*)",    "dest": "/index.html"              }
  ]
}

